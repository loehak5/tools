{
    "manifest_version": 3,
    "name": "Instagram Cookie Exporter",
    "version": "1.0.0",
    "description": "Export Instagram cookies and account metadata for easy account management",
    "permissions": [
        "cookies",
        "storage",
        "activeTab",
        "downloads",
        "scripting"
    ],
    "host_permissions": [
        "https://*.instagram.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}